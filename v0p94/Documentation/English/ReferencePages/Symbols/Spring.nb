Cell[BoxData[
 RowBox[{
  FractionBox["1", 
   SuperscriptBox["l", "2"]], 
  RowBox[{"k", " ", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "l"}], "+", 
